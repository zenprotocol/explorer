    "supertest": "^4.0.2",
    "tap-bail": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "testdouble": "^3.11.0"
  },
  "engines": {
    "node": "10.x"
  },
  "nyc": {
    "include": [
      "server/**"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
