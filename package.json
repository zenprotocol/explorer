{
  "name": "explorer",
  "version": "1.3.1",
  "description": "Zen Protocol block explorer",
  "main": "server/index.js",
  "scripts": {
    "start": "npm run server:start",
    "dev": "npm-run-all -s docker:up client:start",
    "dev:all": "npm-run-all -s docker:up:all client:start",
    "server:start": "node server/index.js",
    "server:dev": "nodemon --inspect=0.0.0.0:5858 -w ./server --legacy-watch server/index.js",
    "heroku-postbuild": "npm run heroku:client:build",
    "heroku-release": "npm run heroku:db:migrate && npm run heroku:db:seed",
    "heroku:db:migrate": "npx sequelize db:migrate",
    "heroku:db:seed": "npx sequelize db:seed:all",
    "heroku:client:build": "npm run client:build",
    "docker:up": "docker-compose up -d",
    "docker:up:all": "docker-compose -f docker-compose.yml -f docker-compose.optional.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.yml -f docker-compose.optional.yml stop",
    "docker:logs": "docker-compose -f docker-compose.yml -f docker-compose.optional.yml logs -f --tail=20",
    "setup": "node build_scripts/setup.js && npm install",
    "setup:db": "npm run setup:db:test && npm run setup:db:development",
    "setup:db:test": "cross-env NODE_ENV=test npx sequelize db:create && cross-env NODE_ENV=test npx sequelize db:migrate",
    "setup:db:development": "npx sequelize db:create && npx sequelize db:migrate",
    "pretest": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "test": "npm-run-all -p test:server test:worker client:lib:test",
    "test:server": "cross-env NODE_ENV=test tape 'server/**/*.test.js' | tap-bail | tap-spec",
    "test:worker": "cross-env NODE_ENV=test tape 'worker/**/*.test.js' | tap-bail | tap-spec",
    "test:coverage": "cross-env NODE_ENV=test nyc tape server/**/*.test.js",
    "client:start": "cross-env PORT=3001 react-scripts start",
    "client:build": "react-scripts build",
    "client:test": "react-scripts test",
    "client:lib:test": "babel-tape-runner src/lib/**/**/*.test.js | tap-bail | tap-spec",
    "client:eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenprotocol/explorer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zenprotocol/explorer/issues"
  },
  "homepage": "http://mywebsite.com/public",
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@zen/zenjs": "0.0.21",
    "asset-require-hook": "^1.2.0",
    "axios": "^0.18.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "balloon-css": "^0.5.0",
    "bech32": "^1.1.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "bull": "^3.4.2",
    "classnames": "^2.2.6",
    "clipboard-polyfill": "^2.7.0",
    "colors": "^1.3.2",
    "common-tags": "^1.8.0",
    "compression": "^1.7.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "deepmerge": "^2.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "helmet": "^3.12.1",
    "highcharts": "^6.1.2",
    "http-status": "^1.2.0",
    "ignore-styles": "^5.0.1",
    "js-file-download": "^0.4.4",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "node-sass": "^4.10.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.6.3",
    "react-dropdown": "^1.6.2",
    "react-highcharts": "^16.0.2",
    "react-highlight": "^0.12.0",
    "react-loadable": "^5.5.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-table": "^6.8.6",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "slack-node": "^0.1.8",
    "url-loader": "^1.0.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.3.2",
    "@types/tape": "^4.2.32",
    "babel-tape-runner": "^2.0.1",
    "blue-tape": "^1.0.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.5",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0",
    "tap-bail": "^1.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "testdouble": "^3.8.2"
  },
  "nyc": {
    "include": [
      "server/**"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
